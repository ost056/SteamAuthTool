{
  "name": "steam-auth-tool",
  "version": "1.4.0",
  "description": "Steam Auth Tool",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "author": "ost56",
  "license": "MIT",
  "devDependencies": {
    "electron": "^25.9.8"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "bottleneck": "^2.19.5",
    "cheerio": "^1.0.0-rc.12",
    "electron-squirrel-startup": "^1.0.0",
    "hpagent": "^1.2.0",
    "jsqr": "^1.4.0",
    "request": "^2.88.2",
    "steam-session": "^1.7.1",
    "steam-totp": "^2.1.2",
    "steamcommunity": "^3.46.1",
    "steamstore": "^2.3.0",
    "systeminformation": "^5.23.5"
  },
  "build": {
    "appId": "com.ost56.steam-auth-tool",
    "productName": "Steam Auth Tool",
    "asar": true,
    "files": [
      "!app.js",
      "!ui",
      "!.vscode",
      "!.gitignore",
      "!forge.config.js",
      "!vetur.config.js",
      "!test.json",
      "!shema.json",
      "!data",
      "!states.json",
      "!backup",
      "!node_modules/@electron",
      "!node_modules/electron",
      "!dist"
    ],
    "directories": {
      "output": "dist"
    },
    "mac": {
      "icon": "logo.icns",
      "target": ["dir", "zip"]
    },
    "win": {
      "icon": "logo.ico",
      "target": ["dir", "zip"]
    },
    "linux": {
      "icon": "logo.png",
      "target": ["dir", "zip"]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
